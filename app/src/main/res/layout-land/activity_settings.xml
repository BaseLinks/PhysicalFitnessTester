    <TableLayout
        android:id="@+id/prev_pageview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view3">

        <TableRow
            android:id="@+id/read_radio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/read_radio_button"
                style="@style/SettingButton"
                android:text="读取系数" />

            <TextView
                android:id="@+id/radio_textview"
                style="@style/SettingTextButton"
                android:ems="10"
                android:text="Name"
                android:textColor="#FFFFFF" />
        </TableRow>

        <TableRow
            android:id="@+id/check_weight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/calibrate_radio_button"
                style="@style/SettingButton"
                android:text="校准体重" />

            <EditText
                android:id="@+id/radio_edittext"
                style="@style/SettingTextButton"
                android:ems="10"
                android:gravity="center"
                android:hint="0-5000"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:text="Name"
                android:textColor="#FFFFFF" />
        </TableRow>

        <TableRow
            android:id="@+id/test"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/test_button"
                style="@style/SettingButton"
                android:text="测　试" />

            <TextView
                android:id="@+id/weight_textview"
                style="@style/SettingTextButton"
                android:ems="10"
                android:text="Name"
                android:textColor="#FFFFFF" />
        </TableRow>

        <TableRow
            android:id="@+id/time_and_ad"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/time_setting_button"
                style="@style/SettingButton"
                android:text="时间设置" />


            <Button
                android:id="@+id/ad_text_setting"
                style="@style/SettingButton"
                android:text="广告文本设置"
                android:textSize="50sp" />
        </TableRow>

        <TableRow
            android:id="@+id/volume"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/volume_setting"
                style="@style/SettingButton"
                android:text="音量设置" />


            <android.support.constraint.ConstraintLayout
                android:id="@+id/volume_constrainlayout"
                style="@style/SettingButton"
                android:background="@android:color/transparent">

                <Button
                    android:id="@+id/volume_add"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/_120_settings_volume"
                    android:onClick="onClick"
                    android:text="+"
                    android:textAllCaps="false"
                    android:textSize="65sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/volume_textview"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="8dp"
                    android:gravity="center_vertical|end"
                    android:onClick="onClick"
                    android:text="100"
                    android:textColor="@android:color/white"
                    android:textSize="60sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/volume_percent_textview"
                    app:layout_constraintStart_toEndOf="@+id/volume_jian"
                    app:layout_constraintTop_toTopOf="parent" />


                <TextView
                    android:id="@+id/volume_percent_textview"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginBottom="8dp"
                    android:gravity="center"
                    android:text="%"
                    android:textAlignment="viewEnd"
                    android:textColor="@android:color/white"
                    android:textSize="30sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/volume_add"
                    app:layout_constraintTop_toTopOf="parent" />

                <Button
                    android:id="@+id/volume_jian"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/_120_settings_volume"
                    android:onClick="onClick"
                    android:padding="0dp"
                    android:text="-"
                    android:textAllCaps="false"
                    android:textSize="65sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </android.support.constraint.ConstraintLayout>
        </TableRow>

        <TableRow
            android:id="@+id/next_and_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/next_page_button"
                style="@style/SettingButton"
                android:text="下一页" />

            <Button
                android:id="@+id/back_button"
                style="@style/SettingButton"
                android:onClick="onClick"
                android:text="返回" />
        </TableRow>
    </TableLayout>

    <TableLayout
        android:id="@+id/next_pageview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view3">

        <TableRow
            android:id="@+id/update_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/update_apk_button"
                style="@style/SettingButton"
                android:text="系统升级" />

            <Button
                android:id="@+id/upload_data"
                style="@style/SettingButton"
                android:text="上传数据" />
        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/wifi_button"
                style="@style/SettingButton"
                android:text="WIFI" />

            <ToggleButton
                android:id="@+id/wifi_ctrl_button"
                style="@style/SettingButton"
                android:text="开"
                android:textOff="关闭"
                android:textOn="打开" />
        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Button
                style="@style/SettingButton"
                android:text="" />

            <Button
                style="@style/SettingButton"
                android:text="" />
        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Button
                style="@style/SettingButton"
                android:text=""
                android:textSize="50sp" />


            <Button
                style="@style/SettingButton"
                android:text=""
                android:textSize="50sp" />
        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                style="@style/SettingButton"
                android:text="" />

            <TextView
                style="@style/SettingButton"
                android:text="" />
        </TableRow>

        <TableRow
            android:id="@+id/prev_and_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/prev_page_button"
                style="@style/SettingButton"
                android:text="上一页" />

            <Button
                android:id="@+id/back2_button"
                style="@style/SettingButton"
                android:text="返回" />
        </TableRow>


    </TableLayout>

